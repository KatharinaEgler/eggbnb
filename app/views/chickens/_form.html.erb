<%= simple_form_for @chicken do |f| %>
  <%= f.input :name %>
  <%= f.input :age %>
  <%= f.input :address %>
  <%= f.input :price,
              hint: ("Please add your price in dollars")%>
  <%= f.input :egg_volume %>
  <%= f.input :egg_color,
               collection: [ "white", "brown", "blue", "green", "pink" ] %>
  <%= f.input :description %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>

    <%= f.button :submit, class: 'btn btn-primary' %>
<% end %>
